    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@dayflow/core": "workspace:*",
    "ng-packagr": "^18.2.1",
    "rimraf": "catalog:",
    "rxjs": "^7.8.1",
    "tslib": "catalog:",
    "typescript": "~5.5.0",
    "zone.js": "~0.14.10"
  },
  "peerDependencies": {
    "@angular/common": ">=14.0.0",
    "@angular/core": ">=14.0.0",
    "@dayflow/core": "workspace:*"
