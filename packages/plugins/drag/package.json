{
  "name": "@dayflow/plugin-drag",
  "version": "1.0.0",
  "description": "Drag plugin for DayFlow calendar - event move, resize, and create via mouse/touch",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "calendar",
    "drag",
    "drop",
    "resize",
    "dayflow",
    "plugin"
  ],
  "author": "Jayce Li",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && rollup -c",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@dayflow/core": "^3.0.0"
  },
  "devDependencies": {
    "@dayflow/core": "workspace:*",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "rollup": "^4.29.1",
    "rollup-plugin-dts": "^6.3.0",
    "temporal-polyfill": "^0.3.0",
    "typescript": "^5.7.2"
  }
}
