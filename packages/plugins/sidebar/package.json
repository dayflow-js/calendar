{
  "name": "@dayflow/plugin-sidebar",
  "version": "1.0.2",
  "description": "Sidebar plugin for DayFlow calendar",
  "keywords": [
    "calendar",
    "dayflow",
    "plugin",
    "sidebar"
  ],
  "license": "MIT",
  "author": "Jayce Li",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json && rollup -c",
    "clean": "rimraf dist node_modules",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@dayflow/blossom-color-picker": "catalog:",
    "@dayflow/core": "workspace:*",
    "@rollup/plugin-commonjs": "catalog:",
    "@rollup/plugin-node-resolve": "catalog:",
    "@rollup/plugin-typescript": "catalog:",
    "preact": "catalog:",
    "rimraf": "catalog:",
    "rollup": "catalog:",
    "rollup-plugin-dts": "catalog:",
    "temporal-polyfill": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "@dayflow/core": "workspace:*"
  }
}
